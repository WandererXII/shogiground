{
  "name": "shogiground",
  "version": "0.8.6",
  "description": "lishogi.org shogi ui",
  "type": "module",
  "module": "esm/shogiground.js",
  "types": "esm/shogiground.d.ts",
  "exports": {
    ".": {
      "types": "./esm/shogiground.d.ts",
      "import": "./esm/shogiground.js"
    },
    "./*": {
      "types": "./esm/*.d.ts",
      "import": "./esm/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "esm/*"
      ]
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "del-cli": "^6",
    "eslint": "^8",
    "prettier": "^3",
    "rollup": "^2",
    "rollup-plugin-terser": "^7",
    "tslib": "^2",
    "typescript": "^5"
  },
  "scripts": {
    "prepare": "$npm_execpath run compile",
    "precompile": "del esm",
    "compile": "tsc",
    "lint": "eslint",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dist": "rollup --config"
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WandererXII/shogiground"
  },
  "author": "Thibault Duplessis (https://github.com/ornicar); WandererXII - modified for shogi",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/WandererXII/shogiground/issues"
  },
  "keywords": [
    "shogi",
    "lishogi",
    "lishogi.org",
    "shogiboard",
    "ui",
    "typescript"
  ],
  "funding": "https://lishogi.org/patron"
}
