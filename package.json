{
  "name": "shogiground",
  "version": "0.8.4",
  "description": "lishogi.org shogi ui",
  "type": "module",
  "module": "shogiground.js",
  "typings": "shogiground.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "^8",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "eslint": "^8",
    "prettier": "^3",
    "rollup": "^2",
    "rollup-plugin-terser": "^7",
    "tslib": "^2",
    "typescript": "^5"
  },
  "scripts": {
    "prepare": "$npm_execpath run compile",
    "compile": "tsc --outDir . --sourceMap --declaration",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dist": "rollup --config"
  },
  "files": [
    "/*.js",
    "/*.d.ts",
    "/*.js.map",
    "/src/*.ts",
    "/dist/*.js",
    "!/rollup.config.js"
  ],
  "repository": "https://github.com/WandererXII/shogiground",
  "keywords": [
    "shogi",
    "lishogi",
    "lishogi.org",
    "shogiboard",
    "ui",
    "typescript"
  ],
  "author": "Thibault Duplessis (https://github.com/ornicar); WandererXII - modified for shogi",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/WandererXII/shogiground/issues"
  },
  "funding": "https://lishogi.org/patron"
}
